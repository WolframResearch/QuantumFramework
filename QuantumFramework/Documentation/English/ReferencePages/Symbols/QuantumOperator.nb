Cell[TextData[{
 "Show Braid satisfies the Yang-Baxter equation (i.e., ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"R", "\[CircleTimes]", "I"}], ")"}], 
     RowBox[{"(", 
      RowBox[{"I", "\[CircleTimes]", "R"}], ")"}], 
     RowBox[{"(", 
      RowBox[{"R", "\[CircleTimes]", " ", "I"}], ")"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"I", "\[CircleTimes]", "R"}], ")"}], 
     RowBox[{"(", 
      RowBox[{"R", "\[CircleTimes]", "I"}], ")"}], 
     RowBox[{"(", 
      RowBox[{"I", "\[CircleTimes]", "R"}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"0d3c6c2b-e49f-450a-8e0d-9b0fbbc8e716"],
