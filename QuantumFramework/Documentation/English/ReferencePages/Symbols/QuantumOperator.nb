Cell[TextData[{
 "Quantum operator is a quantum state, with an order. The order can be a \
nested list as ",
 Cell[BoxData[
  RowBox[{"{", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      SubscriptBox[
       StyleBox["output", "TI"], "1"], ",", 
      SubscriptBox[
       StyleBox["output", "TI"], "2"], ",", "\[Ellipsis]"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox[
       StyleBox["input", "TI"], "1"], ",", 
      SubscriptBox[
       StyleBox["input", "TI"], "2"], ",", "\[Ellipsis]"}], "}"}]}], "}"}]], 
  "InlineFormula",ExpressionUUID->"ac3a8444-37dd-4bad-aa7c-8837a2fb38dc"],
 " or a rule as ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     SubscriptBox[
      StyleBox["input", "TI"], 
      StyleBox["1", "TR"]], ",", 
     SubscriptBox[
      StyleBox["input", "TI"], 
      StyleBox["2", "TR"]], ",", "\[Ellipsis]"}], "}"}], "\[Rule]", 
   RowBox[{"{", 
    RowBox[{
     SubscriptBox[
      StyleBox["output", "TI"], 
      StyleBox["1", "TR"]], ",", 
     SubscriptBox[
      StyleBox["output", "TI"], 
      StyleBox["2", "TR"]], ",", "\[Ellipsis]"}], "}"}]}]], "InlineFormula",
  ExpressionUUID->"94aa4b3a-5b94-4ad6-a636-255e570b04d7"],
 "."
}], "Notes",
