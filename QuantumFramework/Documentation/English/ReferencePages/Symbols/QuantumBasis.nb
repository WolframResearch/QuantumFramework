 RowBox[{"Show", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"QuantumState", "[", "\"\<UniformMixture\>\"", "]"}], "[", 
    "\"\<BlochPlot\>\"", "]"}], ",", 
   RowBox[{"Graphics3D", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Opacity", "[", ".5", "]"}], ",", "Red", ",", 
      RowBox[{"Tetrahedron", "[", 
       RowBox[{"pnts3", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "]"}], ",", "Blue", ",", 
      RowBox[{"Tetrahedron", "[", 
       RowBox[{"pnts3", "[", 
        RowBox[{"[", "2", "]"}], "]"}], "]"}]}], "}"}], "]"}]}], 
  "]"}]], "Input",
