Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"\[Psi]", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Cos", "[", 
        RowBox[{"\[Theta]", "/", "2"}], "]"}], ",", 
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"\[ImaginaryI]", " ", "\[Phi]"}]], 
        RowBox[{"Sin", "[", 
         RowBox[{"\[Theta]", "/", "2"}], "]"}]}]}], "}"}]}], "}"}], ",", 
   RowBox[{"FullSimplify", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Conjugate", "[", "\[Psi]", "]"}], ".", 
      RowBox[{"PauliMatrix", "[", "1", "]"}], ".", "\[Psi]"}], ",", 
