Cell[BoxData[{
 RowBox[{"ClearAll", "[", "BlochVector", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"BlochVector", "[", 
   RowBox[{"stateVector_", "?", "VectorQ"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], ",", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], "=", "stateVector"}], ";", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"2", 
        RowBox[{"Re", "[", 
         RowBox[{"\[Beta]", " ", 
          RowBox[{"Conjugate", "@", "\[Alpha]"}]}], "]"}]}], ",", 
       RowBox[{"2", 
        RowBox[{"Im", "[", 
         RowBox[{"\[Beta]", " ", 
          RowBox[{"Conjugate", "@", "\[Alpha]"}]}], "]"}]}], ",", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"Abs", "[", "\[Alpha]", "]"}], "2"], "-", 
        SuperscriptBox[
         RowBox[{"Abs", "[", "\[Beta]", "]"}], "2"]}]}], "}"}]}]}], 
   "]"}]}]}], "Input",
 CellChangeTimes->{{3.972647561062148*^9, 3.972647606011798*^9}, {
