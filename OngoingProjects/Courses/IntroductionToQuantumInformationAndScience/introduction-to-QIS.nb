Cell[TextData[{
 "From ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       OverscriptBox["a", "\[RightVector]"], ".", 
       OverscriptBox["\[Sigma]", "\[RightVector]"]}], ")"}], 
     RowBox[{"(", 
      RowBox[{
       OverscriptBox["b", "\[RightVector]"], ".", 
       OverscriptBox["\[Sigma]", "\[RightVector]"]}], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        OverscriptBox["a", "\[RightVector]"], ".", 
        OverscriptBox["b", "\[RightVector]"]}], ")"}], 
      "\[DoubleStruckCapitalI]"}], "+", 
     RowBox[{
      RowBox[{"\[ImaginaryI]", "(", 
       RowBox[{
        OverscriptBox["a", "\[RightVector]"], "\[Times]", 
        OverscriptBox["b", "\[RightVector]"]}], ")"}], ".", 
      OverscriptBox["\[Sigma]", "\[RightVector]"]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e01dc827-dad7-4d86-bad3-ac9249cae069"],
 ", we get ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       OverscriptBox["n", "\[RightVector]"], ".", 
       OverscriptBox["\[Sigma]", "\[RightVector]"]}], ")"}], "2"], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       OverscriptBox["n", "\[RightVector]"], ".", 
       OverscriptBox["n", "\[RightVector]"]}], ")"}], "=", 
     "\[DoubleStruckCapitalI]"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e2290c59-5e2c-42c2-a30a-aa3ceb2c8fdb"],
 " where ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["n", "\[RightVector]"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d45954b3-947d-40e9-81cd-71758ce57c49"],
 " is a unit vector. This implies ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       OverscriptBox["n", "\[RightVector]"], ".", 
       OverscriptBox["\[Sigma]", "\[RightVector]"]}], ")"}], 
     RowBox[{"2", "m"}]], "=", "\[DoubleStruckCapitalI]"}], TraditionalForm]],
  
  FormatType->TraditionalForm,ExpressionUUID->
  "d9c67091-6e29-4158-8902-927d92d54237"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       OverscriptBox["n", "\[RightVector]"], ".", 
       OverscriptBox["\[Sigma]", "\[RightVector]"]}], ")"}], 
     RowBox[{
      RowBox[{"2", "m"}], "+", "1"}]], "=", 
    RowBox[{
     OverscriptBox["n", "\[RightVector]"], ".", 
     OverscriptBox["\[Sigma]", "\[RightVector]"]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8a9b7fc5-6d72-4c81-9523-6f979fccfe4d"],
 ". Using these relations in the power-series expansion of the exponential ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", 
     FractionBox["\[Alpha]", "2"], " ", 
     RowBox[{
      OverscriptBox["n", "\[RightVector]"], ".", 
      OverscriptBox["\[Sigma]", "\[RightVector]"]}]}]], TraditionalForm]],
  ExpressionUUID->"2eb42dca-fef7-4ad2-911c-1d2dab29842c"],
 ", we find: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", 
      FractionBox["\[Alpha]", "2"], " ", 
      RowBox[{
       OverscriptBox["n", "\[RightVector]"], ".", 
       OverscriptBox["\[Sigma]", "\[RightVector]"]}]}]], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"Cos", "[", 
       RowBox[{"\[Alpha]", "/", "2"}], "]"}], "\[DoubleStruckCapitalI]"}], "-", 
     RowBox[{"\[ImaginaryI]", " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"\[Alpha]", "/", "2"}], "]"}], 
      RowBox[{
       OverscriptBox["n", "\[RightVector]"], ".", 
       OverscriptBox["\[Sigma]", "\[RightVector]"]}]}]}]}], TraditionalForm]],
  
  FormatType->TraditionalForm,ExpressionUUID->
  "ad2aa2ca-f2b8-40b7-abac-16dea9da9c9a"],
 "."
}], "Text",
