Cell[TextData[{
 "Another useful way to quantify \[OpenCurlyDoubleQuote]mixedness\
\[CloseCurlyDoubleQuote] is the von Neumann entropy, which gives an \
information-theoretic meaning to how uncertain (or mixed) a quantum state is. \
It is defined as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalS]", "=", 
    RowBox[{"-", 
     RowBox[{"Tr", "[", 
      RowBox[{"\[Rho]", ".", 
       RowBox[{"Log2", "[", "\[Rho]", "]"}]}], "]"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "dd6f8441-ae2f-4597-b903-1dc2399e6857"],
 " where the logarithm is typically taken in base 2 (giving entropy in bits) \
or base \[ExponentialE] (giving nats)."
}], "Text",
 CellChangeTimes->{{3.973294354007699*^9, 3.973294416602672*^9}, {
  3.97329677706074*^9, 3.973296796001781*^9}, {3.973296837323769*^9, 
  3.973296838182811*^9}, {3.973297495135964*^9, 
  3.973297495136023*^9}},ExpressionUUID->"d645051d-f673-4876-a5b8-\
f04aa0842f26"],

Cell[TextData[{
 "Compute the von Neumann entropy for the density matrix ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{"\[DoubleStruckCapitalI]", "+", 
       RowBox[{
        OverscriptBox["r", "\[RightVector]"], ".", 
        OverscriptBox["\[Sigma]", "\[RightVector]"]}]}], ")"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5adcd093-12e5-4a09-9658-cafdc9a66e93"],
 ":"
}], "Text",
 CellChangeTimes->{{3.973296863045779*^9, 
  3.973296884986066*^9}},ExpressionUUID->"63445373-5ba1-4a04-8f13-\
998500f9d5ba"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[Rho]", "=", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"1", ",", "rx", ",", "ry", ",", "rz"}], "}"}], ".", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"PauliMatrix", "[", "j", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "0", ",", "3"}], "}"}]}], "]"}]}]}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"Tr", "[", 
       RowBox[{"\[Rho]", ".", 
        RowBox[{"MatrixLog", "[", "\[Rho]", "]"}]}], "]"}]}], "/", 
     RowBox[{"Log", "[", "2", "]"}]}]}], "]"}], "//", 
  "FullSimplify"}]], "Input",
 CellChangeTimes->{{3.973294227707447*^9, 3.9732942354265347`*^9}, {
  3.973297348985311*^9, 3.9732973694580708`*^9}},
 CellLabel->
  "In[110]:=",ExpressionUUID->"71bde014-bc61-4d06-a00b-81e81d605f47"],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{
    RowBox[{"2", " ", 
     SqrtBox[
      RowBox[{
       SuperscriptBox["rx", "2"], "+", 
       SuperscriptBox["ry", "2"], "+", 
       SuperscriptBox["rz", "2"]}]], " ", 
     RowBox[{"ArcTanh", "[", 
      SqrtBox[
       RowBox[{
        SuperscriptBox["rx", "2"], "+", 
        SuperscriptBox["ry", "2"], "+", 
        SuperscriptBox["rz", "2"]}]], "]"}]}], "+", 
    RowBox[{"Log", "[", 
     RowBox[{
      FractionBox["1", "4"], "-", 
      RowBox[{
       FractionBox["1", "4"], " ", 
       SqrtBox[
        RowBox[{
         SuperscriptBox["rx", "2"], "+", 
         SuperscriptBox["ry", "2"], "+", 
         SuperscriptBox["rz", "2"]}]]}]}], "]"}], "+", 
    RowBox[{"Log", "[", 
     RowBox[{"1", "+", 
      SqrtBox[
       RowBox[{
        SuperscriptBox["rx", "2"], "+", 
        SuperscriptBox["ry", "2"], "+", 
        SuperscriptBox["rz", "2"]}]]}], "]"}]}], 
   RowBox[{"Log", "[", "4", "]"}]]}]], "Output",
 CellChangeTimes->{{3.9732942318038282`*^9, 3.9732942359560623`*^9}, {
  3.973297359365398*^9, 3.973297369748015*^9}},
 CellLabel->
  "Out[110]=",ExpressionUUID->"edf5663b-9eec-4d1a-9517-f24848425314"]
}, Open  ]],

