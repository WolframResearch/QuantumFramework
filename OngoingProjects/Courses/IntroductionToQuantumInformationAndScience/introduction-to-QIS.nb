Cell["\<\
Let\[CloseCurlyQuote]s generalize the function BlochVector we introduced \
before so that it also accepts density matrices and returns the corresponding \
Bloch vector.\
\>", "Text",
 CellChangeTimes->{{3.973293470040936*^9, 
  3.973293509638698*^9}},ExpressionUUID->"2b8eb23e-cca1-460f-9d2c-\
8e60d716f906"],

Cell[BoxData[{
 RowBox[{"ClearAll", "[", "BlochVector", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"BlochVector", "[", 
   RowBox[{"v_", "?", "VectorQ"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], ",", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], "=", "v"}], ";", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"2", 
        RowBox[{"Re", "[", 
         RowBox[{"\[Beta]", " ", 
          RowBox[{"Conjugate", "@", "\[Alpha]"}]}], "]"}]}], ",", 
       RowBox[{"2", 
        RowBox[{"Im", "[", 
         RowBox[{"\[Beta]", " ", 
          RowBox[{"Conjugate", "@", "\[Alpha]"}]}], "]"}]}], ",", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"Abs", "[", "\[Alpha]", "]"}], "2"], "-", 
        SuperscriptBox[
         RowBox[{"Abs", "[", "\[Beta]", "]"}], "2"]}]}], "}"}]}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"BlochVector", "[", 
   RowBox[{"\[Rho]_", "?", "MatrixQ"}], "]"}], ":=", 
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"Re", "@", 
     RowBox[{"Chop", "@", 
      RowBox[{"Tr", "[", 
       RowBox[{"\[Rho]", ".", 
        RowBox[{"PauliMatrix", "[", "j", "]"}]}], "]"}]}]}], ",", 
    RowBox[{"{", 
     RowBox[{"j", ",", "3"}], "}"}]}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.973293460418419*^9, 3.973293464806991*^9}},
 CellLabel->"In[84]:=",ExpressionUUID->"203fbbc3-c4d1-4543-a59f-d8f68a4aa603"],

