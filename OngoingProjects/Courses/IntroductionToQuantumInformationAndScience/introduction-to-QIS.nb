Cell[TextData[{
 "The eigenstates of Pauli-Y are also denoted by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"R"},
     "Ket"], "=", 
    TemplateBox[{
      SubscriptBox["y", "+"]},
     "Ket"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f7d99e2b-8771-49c0-b500-5b3770849e7b"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"L"},
     "Ket"], "=", 
    TemplateBox[{
      SubscriptBox["y", "-"]},
     "Ket"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a8fa9ea5-1615-43c9-8671-b473c7881bc3"],
 ", following a standard convention in optics and photon polarization."
}], "Text",
