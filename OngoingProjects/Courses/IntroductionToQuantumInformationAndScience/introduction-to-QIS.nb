 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["n", "\[RightVector]"], ".", 
    OverscriptBox["\[Sigma]", "\[RightVector]"]}], TraditionalForm]],
  ExpressionUUID->"59ac7136-6939-46e0-ba6a-bc0f0d63c0a8"],
 ", a standard choice of eigenstates is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{
      SubscriptBox["n", "+"]},
     "Ket"], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Cos", "[", 
       RowBox[{"\[Theta]", "/", "2"}], "]"}], ",", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", "\[ImaginaryI]\[Phi]"], 
       RowBox[{"Sin", "[", 
        RowBox[{"\[Theta]", "/", "2"}], "]"}]}]}], "}"}]}], TraditionalForm]],
  
  FormatType->TraditionalForm,ExpressionUUID->
  "bb1d84c4-da70-45d4-822c-9e08e3ba429c"],
