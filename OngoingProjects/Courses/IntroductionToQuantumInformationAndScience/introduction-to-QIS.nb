Cell["\<\
Now the motivation for having \[Theta]/2 in the state vector is clear: if we \
used \[Theta] instead of \[Theta]/2, the components of the Bloch vector would \
involve 2\[Theta], and we would obtain an extra factor of 2 in the spherical \
angles on the Bloch sphere.\
\>", "Text",
 CellChangeTimes->{{3.9726486604304028`*^9, 
  3.972648735167213*^9}},ExpressionUUID->"8dce2f23-57f3-4448-863a-\
98f34bba1dec"],

Cell[TextData[{
 "Calculate the Bloch vector for the state",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"\[Psi]"},
     "Ket"], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Cos", "[", "\[Theta]", "]"}], ",", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"\[ImaginaryI]", " ", "\[Phi]"}]], 
       RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"d996a6ae-8e9a-4989-9482-8a24b043e11f"],
 ":"
}], "Text",
 CellChangeTimes->{{3.973781087192808*^9, 3.973781092451818*^9}, 
   3.97378157697733*^9},ExpressionUUID->"48056405-1a62-4117-989e-\
6278f3c6063a"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"FullSimplify", "[", 
  RowBox[{
   RowBox[{"BlochVector", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Cos", "[", "\[Theta]", "]"}], ",", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"\[ImaginaryI]", " ", "\[Phi]"}]], 
       RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], "}"}], "]"}], ",", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"\[Theta]", "|", "\[Phi]"}], ")"}], "\[Element]", "Reals"}]}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.972648687429347*^9, 3.972648689669279*^9}, {
  3.97378089362177*^9, 3.9737808967304564`*^9}},
 CellLabel->
  "In[247]:=",ExpressionUUID->"a5d4c15c-8841-4106-bdf5-7cdcd354c8de"],

Cell[BoxData[
 RowBox[{"{", 
