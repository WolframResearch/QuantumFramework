Cell[BoxData[
 RowBox[{"u", ".", 
  RowBox[{"ConjugateTranspose", "[", "u", "]"}]}]], "Input",
 CellLabel->"In[52]:=",ExpressionUUID->"795741b6-847d-45ac-8b33-7f64f9d05d55"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"1.`", "\[VeryThinSpace]", "-", 
      RowBox[{"1.3025279425631393`*^-17", " ", "\[ImaginaryI]"}]}], ",", 
     RowBox[{"5.551115123125783`*^-17", "+", 
      RowBox[{"1.6653345369377348`*^-16", " ", "\[ImaginaryI]"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"5.551115123125783`*^-17", "-", 
      RowBox[{"1.6653345369377348`*^-16", " ", "\[ImaginaryI]"}]}], ",", 
     RowBox[{"0.9999999999999998`", "\[VeryThinSpace]", "+", 
      RowBox[{"2.814073559692696`*^-17", " ", "\[ImaginaryI]"}]}]}], "}"}]}], 
  "}"}]], "Output",
 CellChangeTimes->{3.972592975291543*^9, 3.972678181652668*^9, 
  3.972678412894166*^9, 3.97274385892066*^9, 3.973006492128662*^9},
 CellLabel->"Out[52]=",ExpressionUUID->"a142146a-34a6-4d03-816f-fe2ff5eb204b"]
}, Open  ]],

