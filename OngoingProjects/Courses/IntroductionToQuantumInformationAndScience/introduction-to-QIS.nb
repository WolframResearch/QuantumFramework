 RowBox[{
  RowBox[{"ClearAll", "[", "A", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"a", "=", 
      RowBox[{"Array", "[", 
       RowBox[{
        RowBox[{
         SubscriptBox["A", "##"], "&"}], ",", 
        RowBox[{"{", 
