   RowBox[{
    RowBox[{"\[LeftAngleBracket]", "A", "\[RightAngleBracket]"}], "=", 
    RowBox[{"Tr", "[", 
     RowBox[{"\[Rho]", ".", "A"}], "]"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4691afdf-8659-4444-ac7f-06ab0c26f988"],
