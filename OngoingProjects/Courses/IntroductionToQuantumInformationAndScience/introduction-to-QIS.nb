     RowBox[{
      RowBox[{"{", 
       RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], "=", "stateVector"}], ";", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"2", 
        RowBox[{"Re", "[", 
         RowBox[{"\[Beta]", " ", 
          RowBox[{"Conjugate", "@", "\[Alpha]"}]}], "]"}]}], ",", 
       RowBox[{"2", 
        RowBox[{"Im", "[", 
         RowBox[{"\[Beta]", " ", 
          RowBox[{"Conjugate", "@", "\[Alpha]"}]}], "]"}]}], ",", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"Abs", "[", "\[Alpha]", "]"}], "2"], "-", 
        SuperscriptBox[
         RowBox[{"Abs", "[", "\[Beta]", "]"}], "2"]}]}], "}"}]}]}], 
   "]"}]}]}], "Input",
