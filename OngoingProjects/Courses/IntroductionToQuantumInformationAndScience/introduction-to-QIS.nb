Cell[TextData[{
 "This definition isn\[CloseCurlyQuote]t arbitrary: for a normalized pure \
state ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"\[Psi]"},
    "Ket"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ac681674-acae-4fe4-9097-f1595b84e479"],
 ", the three component of Bloch vector are exactly the expectation-value of \
Pauli operators ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"\[Psi]"},
     "Bra"], 
    SubscriptBox["\[Sigma]", "j"], 
    TemplateBox[{"\[Psi]"},
     "Ket"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "66cd541a-441b-4cca-9a0e-fc20699a054c"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "x"}], ",", "y", ",", "z"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e684553a-fa00-4289-9f70-099db2d43c49"],
 ". That\[CloseCurlyQuote]s why the Bloch vector is operational: it\
\[CloseCurlyQuote]s what you can measure."
}], "Text",
 CellChangeTimes->{{3.973711902987377*^9, 3.973711912345145*^9}, {
  3.973711943870818*^9, 
  3.9737120056151752`*^9}},ExpressionUUID->"db2b65d9-6089-430f-8ddb-\
3e345df1b057"],

Cell["\<\
Verify that the norm of the Bloch vector is equal to the squared norm of the \
state vector:\
\>", "Text",
 CellChangeTimes->{{3.972647798666408*^9, 3.972647807124976*^9}, {
   3.972647839227325*^9, 3.972647843224189*^9}, {3.972647951273323*^9, 
   3.972647957965796*^9}, {3.972742169618805*^9, 3.972742197864358*^9}, 
   3.9732979311398087`*^9},ExpressionUUID->"e605a100-8354-4d46-857f-\
9e6097c37e42"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"stateVector", "=", 
      RowBox[{"{", 
       RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"Norm", "[", 
      RowBox[{"BlochVector", "[", "stateVector", "]"}], "]"}], "==", 
     RowBox[{
      RowBox[{"Norm", "[", "stateVector", "]"}], "^", "2"}]}]}], "]"}], "//", 
  "FullSimplify"}]], "Input",
