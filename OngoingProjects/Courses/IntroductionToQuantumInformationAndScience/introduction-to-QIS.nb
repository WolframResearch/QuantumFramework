Cell[BoxData[{
 RowBox[{"ClearAll", "[", "BlochVector", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"BlochVector", "[", 
   RowBox[{"stateVector_", "?", "VectorQ"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], ",", 
