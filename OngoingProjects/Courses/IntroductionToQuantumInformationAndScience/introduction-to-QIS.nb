Cell[BoxData[
 RowBox[{
  RowBox[{"Chop", "@", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"v1", ".", 
        RowBox[{"Conjugate", "[", "v1", "]"}]}], ",", 
       RowBox[{"v1", ".", 
        RowBox[{"Conjugate", "[", "v2", "]"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"v2", ".", 
        RowBox[{"Conjugate", "[", "v1", "]"}]}], ",", 
       RowBox[{"v2", ".", 
        RowBox[{"Conjugate", "[", "v2", "]"}]}]}], "}"}]}], "}"}]}], "==", 
