Cell[TextData[{
 "Let\[CloseCurlyQuote]s focus on a generic qubit state vector ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6f8b068f-2903-4888-82db-5b52d7f523fa"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", ",", 
    RowBox[{"\[Beta]", "\[Element]", "\[DoubleStruckCapitalC]"}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ea426afc-c9e2-41b5-a149-f0070fa49405"],
 ", assumed to be normalized ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"\[LeftBracketingBar]", "\[Alpha]", "\[RightBracketingBar]"}], 
      "2"], "+", 
     SuperscriptBox[
      RowBox[{"\[LeftBracketingBar]", "\[Beta]", "\[RightBracketingBar]"}], 
      "2"]}], "=", "1"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f7ea30a8-1f21-4609-bca0-28f0cd3eb3e9"],
 ". Define a three-component real vector as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["n", "\[RightVector]"], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"2", 
       RowBox[{"Re", "[", 
        RowBox[{
         SuperscriptBox["\[Alpha]", "*"], " ", "\[Beta]"}], "]"}]}], ",", 
      RowBox[{"2", 
       RowBox[{"Im", "[", 
        RowBox[{
         SuperscriptBox["\[Alpha]", "*"], " ", "\[Beta]"}], "]"}]}], ",", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"\[LeftBracketingBar]", "\[Alpha]", "\[RightBracketingBar]"}],
         "2"], "-", 
       SuperscriptBox[
        RowBox[{"\[LeftBracketingBar]", "\[Beta]", "\[RightBracketingBar]"}], 
        "2"]}]}], "}"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a42e7109-1a04-42eb-b8b3-98a86ed0f9d0"],
 ". This vector ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["n", "\[RightVector]"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8e5b3dfa-e343-4c22-ac42-33ac5fe46579"],
 " is called the ",
 StyleBox["Bloch vector",
  FontSlant->"Italic"],
 " associated with the state vector ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"\[Alpha]", ",", " ", "\[Beta]"}], "}"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "14931a1e-f0e4-476f-bdde-689c779bcc2e"],
 "."
}], "Text",
