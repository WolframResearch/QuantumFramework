Cell[CellGroupData[{

Cell["The Bloch Vector: Cartesian and Spherical Coordinates", "Subsubsection",
 CellChangeTimes->{{3.973713556752714*^9, 3.9737135697391644`*^9}, {
  3.9737808467807693`*^9, 
  3.973780862426015*^9}},ExpressionUUID->"b6468acd-a328-4942-ab71-\
bda5f2ee57e9"],

Cell[TextData[{
 "Recall that a pure qubit state, in its most general form, can be written as \
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"\[Psi]"},
     "Ket"], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Cos", "[", 
       RowBox[{"\[Theta]", "/", "2"}], "]"}], ",", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"\[ImaginaryI]", " ", "\[Phi]"}]], 
       RowBox[{"Sin", "[", 
        RowBox[{"\[Theta]", "/", "2"}], "]"}]}]}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"17d6804f-0b76-40ef-9966-a62d425237a4"],
 ". A key result is that the angles \[Theta] and \[Phi] are exactly the usual \
spherical coordinates on the Bloch sphere. In what follows, we will show this \
explicitly by direct computation."
}], "Text",
 CellChangeTimes->{{3.9726484787075453`*^9, 3.972648593372839*^9}, {
  3.972742352837181*^9, 
  3.972742353044685*^9}},ExpressionUUID->"4d10c408-ec9b-4f8d-a3a2-\
0a3c716ccc19"],

Cell[TextData[{
 "Calculate the Bloch vector for the state ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"\[Psi]"},
     "Ket"], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Cos", "[", 
       RowBox[{"\[Theta]", "/", "2"}], "]"}], ",", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"\[ImaginaryI]", " ", "\[Phi]"}]], 
       RowBox[{"Sin", "[", 
        RowBox[{"\[Theta]", "/", "2"}], "]"}]}]}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"49807ea4-67e6-4bc5-a35e-8a1f31a5a44e"],
 ":"
}], "Text",
 CellChangeTimes->{{3.972648606099085*^9, 3.9726486131842213`*^9}, {
  3.9727424959762897`*^9, 
  3.972742503457746*^9}},ExpressionUUID->"9be5e9e7-97dd-4246-92d4-\
5870944493f3"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"FullSimplify", "[", 
  RowBox[{
   RowBox[{"BlochVector", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Cos", "[", 
       RowBox[{"\[Theta]", "/", "2"}], "]"}], ",", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"\[ImaginaryI]", " ", "\[Phi]"}]], 
       RowBox[{"Sin", "[", 
        RowBox[{"\[Theta]", "/", "2"}], "]"}]}]}], "}"}], "]"}], ",", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"\[Theta]", "|", "\[Phi]"}], ")"}], "\[Element]", "Reals"}]}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.972647859426104*^9, 3.972647881656787*^9}, {
  3.973780872143854*^9, 3.973780887708268*^9}},
 CellLabel->
  "In[246]:=",ExpressionUUID->"dd2e1651-4bd5-4603-8555-b6fca814defb"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{
    RowBox[{"Cos", "[", "\[Phi]", "]"}], " ", 
    RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ",", 
   RowBox[{
    RowBox[{"Sin", "[", "\[Theta]", "]"}], " ", 
    RowBox[{"Sin", "[", "\[Phi]", "]"}]}], ",", 
   RowBox[{"Cos", "[", "\[Theta]", "]"}]}], "}"}]], "Output",
 CellChangeTimes->{{3.972647878229411*^9, 3.972647901819813*^9}, 
   3.972647967199095*^9, 3.972648099644895*^9, 3.9726781811315517`*^9, 
   3.972678412840013*^9, 3.9727438581765823`*^9, 3.973006491382983*^9, 
   3.973558201174199*^9, 3.973558297207796*^9, 3.973695886671317*^9, 
   3.973712755992713*^9, 3.973780888067363*^9},
 CellLabel->
  "Out[246]=",ExpressionUUID->"3a071934-dc23-483a-bba9-58b82f39387e"]
}, Open  ]],

Cell["\<\
As one can see, angles \[Theta] and \[Phi] are exactly the standard spherical \
coordinates on the unit sphere.\
\>", "Text",
 CellChangeTimes->{{3.9726482107274637`*^9, 3.9726482220117702`*^9}, {
  3.97264826362792*^9, 3.972648289721092*^9}, {3.972648618680167*^9, 
  3.972648620564136*^9}, {3.972742609830256*^9, 
  3.9727426101993217`*^9}},ExpressionUUID->"edf1157a-a9be-4deb-bc37-\
6ca83d5278e4"],

Cell[TextData[{
 "Transform a point on the unit sphere from spherical coordinates, specified \
by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"1", ",", "\[Theta]", ",", "\[Phi]"}], "}"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7dbd1b9a-5ae1-41e4-a89d-800c4c64257f"],
 ", into Cartesian coordinates:"
}], "Text",
 CellChangeTimes->{{3.97264829523978*^9, 3.9726483091609993`*^9}, {
  3.972648628014204*^9, 3.972648631431939*^9}, {3.972648767120943*^9, 
  3.972648795671617*^9}, {3.972742539895523*^9, 3.9727425567815323`*^9}, {
  3.973781117726379*^9, 3.973781154652614*^9}, {3.973781200003006*^9, 
  3.973781210282311*^9}},ExpressionUUID->"e3609f8d-8b3c-4975-819a-\
386f3571e2a8"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"CoordinateTransform", "[", 
  RowBox[{
   RowBox[{"\"\<Spherical\>\"", "->", "\"\<Cartesian\>\""}], ",", 
   RowBox[{"{", 
    RowBox[{"1", ",", "\[Theta]", ",", "\[Phi]"}], "}"}]}], "]"}]], "Input",
