Cell[BoxData[{
 RowBox[{
  RowBox[{"right", "=", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"1", ",", "\[ImaginaryI]"}], "}"}], "/", 
    SqrtBox["2"]}]}], ";"}], "\[IndentingNewLine]", 
