     RowBox[{"Abs", "[", 
      RowBox[{"{", 
       RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], "]"}], "2"], "==", 
    SuperscriptBox[
     RowBox[{"Abs", "[", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"\[ImaginaryI]", " ", "\[CurlyPhi]"}]], 
