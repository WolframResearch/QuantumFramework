Cell[TextData[{
 "The Bloch vector is represented as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["r", "x"], ",", 
     SubscriptBox["r", "y"], ",", 
     SubscriptBox["r", "z"]}], "}"}], TraditionalForm]],ExpressionUUID->
  "623cda6b-0403-4cda-b26c-17c96ae61c38"],
 " such that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["r", "x", "2"], "+", 
     SubsuperscriptBox["r", "y", "2"], "+", 
     SubsuperscriptBox["r", "z", "2"]}], "<=", "1"}], TraditionalForm]],
  ExpressionUUID->"175060ab-9e55-4e3a-be85-f2c5ad09989f"],
 ". In the spherical coordinates, it is described by three variables ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"r", ",", "\[Theta]", ",", "\[Phi]"}], "}"}], TraditionalForm]],
  ExpressionUUID->"50bebc22-65b8-4846-84cf-d7e9b75a8ec8"],
 " and its Cartesian coordinates are given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"Cos", "[", "\[Phi]", "]"}], " ", 
       RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ",", 
      RowBox[{
       RowBox[{"Sin", "[", "\[Theta]", "]"}], " ", 
       RowBox[{"Sin", "[", "\[Phi]", "]"}]}], ",", 
      RowBox[{"Cos", "[", "\[Theta]", "]"}]}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"3769ada7-038a-4497-ac7f-515a7edea61a"],
 ". "
}], "Text",ExpressionUUID->"d9acbb69-3a07-4ee0-90c9-6a1e9d974c09"],

