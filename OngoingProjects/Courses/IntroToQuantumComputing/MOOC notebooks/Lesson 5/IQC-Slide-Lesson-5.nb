Cell[TextData[{
 "Imposing the condition that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"Cos", "[", 
      FractionBox["\[Theta]", "2"], "]"}], "2"], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"Sin", "[", 
       FractionBox["\[Theta]", "2"], "]"}], "2"], "=", 
     RowBox[{"1", "/", "2"}]}]}], TraditionalForm]],ExpressionUUID->
  "59cd7db3-e196-45b4-b792-737bf49bbb90"],
 " implies that \[Theta] can be ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[PlusMinus]", "\[Pi]"}], "/", "2"}], TraditionalForm]],
  ExpressionUUID->"d094c893-44c0-408b-81ba-7425a227fe88"]
}], "Text",
