    RowBox[{
     RowBox[{
      SuperscriptBox[
       RowBox[{"Cos", "[", 
        FractionBox["\[Theta]", "2"], "]"}], "2"], "==", 
      RowBox[{"1", "/", "2"}]}], "&&", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"Sin", "[", 
        FractionBox["\[Theta]", "2"], "]"}], "2"], "==", 
      RowBox[{"1", "/", "2"}]}], "&&", 
     RowBox[{
      RowBox[{"-", "\[Pi]"}], "<=", "\[Theta]", "<=", "\[Pi]"}]}], ",", 
    "\[Theta]"}], "]"}], "//", "FullSimplify"}]], "Input",
